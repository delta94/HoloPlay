{
  "name": "youtube-audio-player",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "node ./node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "android:run": "export ANDROID_HOME=/home/stephane/Android/Sdk && export PATH=${PATH}:$ANDROID_HOME/tools:$ANDROID_HOME/platform-tools && export JAVA_HOME=/usr/lib/jvm/java-8-oracle && node ./node_modules/react-native/local-cli/cli.js run-android",
    "android:bundle": "mkdir -p .code-push && react-native bundle --platform android --entry-file /app/packages/mobile/index.js --assets-dest .code-push --bundle-output .code-push/index.android.bundle --dev false",
    "android:release": "cd android && ./gradlew assembleRelease",
    "android:release:debug": "cd android && ./gradlew assembleDebug",
    "push:production": "code-push release Youtube-Audio-Player .code-push --deploymentName Production '*'",
    "clean:all": "sudo rm -rf node_modules packages/*/node_modules",
    "clean:log": "rm **/*/*.log",
    "prettify": "prettier --write src/**/*.js",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint": "eslint src",
    "flow": "./node_modules/.bin/flow",
    "static-analysis": "yarn flow && yarn lint"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.3.1",
    "querystring": "^0.2.0",
    "react": "16.8.3",
    "react-native": "0.59.4",
    "react-native-code-push": "^5.5.3",
    "react-native-music-control": "^0.9.9",
    "react-native-video": "^4.3.1",
    "react-navigation": "2.17.0",
    "react-waterfall": "^4.0.1",
    "uuid": "^3.3.2",
    "youtube-search": "^1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-preset-flow": "^6.23.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.15.0",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.91.0",
    "jest": "^24.7.1",
    "metro-react-native-babel-preset": "^0.53.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.1",
    "react-native-dotenv": "^0.2.0",
    "react-test-renderer": "16.6.3",
    "xml2js": "^0.4.19"
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "flow"
  ]
}
