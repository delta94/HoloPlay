{
  "name": "youtube-audio-player",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "node ./node_modules/react-native/local-cli/cli.js start -- --reset-cache",
    "android:run": "./scripts/android-run.sh",
    "android:run:emulator": "./scripts/android-run-emulator.sh",
    "android:release": "cd android && ./gradlew assembleRelease",
    "android:release:debug": "cd android && ./gradlew assembleDebug",
    "push:production": "code-push release-react Youtube-Audio-Player android --deploymentName Production --targetBinaryVersion '*'",
    "clean:log": "rm *.log",
    "prettify": "prettier --write src/**/*.js",
    "precommit-msg": "echo 'Pre-commit checks...' && exit 0",
    "lint": "eslint src",
    "flow": "./node_modules/.bin/flow",
    "static-analysis": "yarn flow && yarn lint",
    "storybook": "storybook start"
  },
  "dependencies": {
    "@react-native-community/async-storage": "^1.3.1",
    "querystring": "^0.2.0",
    "react": "16.8.3",
    "react-native": "0.59.4",
    "react-native-code-push": "^5.5.3",
    "react-native-config": "^0.11.7",
    "react-native-gesture-handler": "^1.1.0",
    "react-native-music-control": "^0.9.9",
    "react-native-paper": "^2.16.0",
    "react-native-quick-actions": "^0.3.10",
    "react-native-reanimated": "^1.0.1",
    "react-native-simple-dialogs": "^1.1.0",
    "react-native-snap-carousel": "^3.8.0",
    "react-native-svg": "^9.4.0",
    "react-native-svg-icon": "^0.8.1",
    "react-native-vector-icons": "^6.5.0",
    "react-native-video": "^4.3.1",
    "react-navigation": "^3.8.1",
    "react-navigation-material-bottom-tabs": "^1.0.0",
    "react-waterfall": "^4.0.1",
    "reanimated-bottom-sheet": "^1.0.0-alpha.8",
    "uuid": "^3.3.2",
    "youtube-search": "^1.1.4"
  },
  "devDependencies": {
    "@babel/core": "^7.4.3",
    "@babel/runtime": "^7.4.3",
    "@storybook/react-native": "^4.1.18",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.7.1",
    "babel-preset-flow": "^6.23.0",
    "concurrently": "^4.1.0",
    "eslint": "^5.12.1",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-config-prettier": "^3.6.0",
    "eslint-plugin-flowtype": "^3.2.1",
    "eslint-plugin-import": "^2.17.2",
    "eslint-plugin-prettier": "^3.0.1",
    "eslint-plugin-react": "^7.12.4",
    "flow-bin": "^0.91.0",
    "jest": "^24.7.1",
    "metro-react-native-babel-preset": "^0.53.1",
    "pre-commit": "^1.2.2",
    "prettier": "^1.16.1",
    "react-native-storybook-loader": "^1.8.0",
    "react-test-renderer": "16.6.3",
    "shelljs": "^0.8.3",
    "xml2js": "^0.4.19"
  },
  "pre-commit": [
    "precommit-msg",
    "lint",
    "flow"
  ]
}
