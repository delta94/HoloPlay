{
  "name": "@youtube-audio-player/mobile",
  "version": "0.0.1",
  "private": false,
  "scripts": {
    "start": "node ../../node_modules/react-native/local-cli/cli.js start",
    "android:run": "node ../../node_modules/react-native/local-cli/cli.js run-android",
    "android:bundle": "mkdir -p .code-push && react-native bundle --platform android --entry-file /app/packages/mobile/index.js --assets-dest .code-push --bundle-output .code-push/index.android.bundle --dev false",
    "android:release": "cd android && ./gradlew assembleRelease",
    "android:release:debug": "cd android && ./gradlew assembleDebug",
    "push:production": "code-push release Youtube-Audio-Player .code-push --deploymentName Production '*'",
    "test": "jest"
  },
  "dependencies": {
    "@youtube-audio-player/components": "0.0.1",
    "@youtube-audio-player/core": "0.0.1",
    "querystring": "^0.2.0",
    "react": "16.6.3",
    "react-native": "0.57.8",
    "react-native-music-control": "^0.9.7",
    "react-native-video": "^4.3.1",
    "react-navigation": "2.17.0"
  },
  "devDependencies": {
    "babel-jest": "23.6.0",
    "jest": "23.6.0",
    "metro-react-native-babel-preset": "0.51.1",
    "react-test-renderer": "16.6.3"
  },
  "jest": {
    "preset": "react-native"
  }
}
